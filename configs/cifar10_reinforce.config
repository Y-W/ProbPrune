[general]
target = train_vanilla
name = cifar10_vanilla

[dataset]
dataset = cifar10

[net_arch]
net_arch = configs/net/resnet18_cifar10.config

[outputs]
output_dir = models/cifar10_vanilla
output_name = resnet18_cifar10
save_interval = 10

[train]
epochs = 180
learning_rate = 1e-1
lr_step_epochs = 90,45
momentum = 0.9
weight_decay = 1e-4
